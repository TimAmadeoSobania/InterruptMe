{
    "name":"InterruptMe",
    "description":"Interrupt your browsing on time wasting sites.",
    "version":"0.1.0",
    "manifest_version":3,
    "content_scripts":[
        {
            "matches":["*://*/*"],
            "css":["interrupt.css"],
            "js":["interrupt.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Interrupt Me"
    }
}